   "source": [
    "import yfinance as yf\n",
    "import pandas as pd\n",
    "\n",
    "nyse_asset = '^NYA'\n",
    "vix_asset = '^VIX'\n",
    "\n",
    "min_year = 2007\n",
    "max_year = 2022"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "07ce30bd",
   "metadata": {},
   "outputs": [],
   "source": [
    "nyse_index = yf.Ticker(f'{nyse_asset}')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "348d9946",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Histórico de 1 ano com intervalo diário\n",
    "start = '2007-01-01'\n",
    "end = '2022-12-31'\n",
    "\n",
    "nyse_history = nyse_index.history(start=start, end=end).reset_index()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "4736e3a0",
   "metadata": {},
   "outputs": [],
   "source": [
    "(nyse_history\n",
    "    .assign(\n",
    "        Year=lambda x: x['Date'].dt.year,\n",
    "        Month=lambda x: x['Date'].dt.month\n",
    "    )\n",
    "    .groupby(['Year', 'Month'])\n",
    "    .mean()\n",
    "    .reset_index()\n",
    "    .filter(items=['Date', 'Year', 'Month', 'Close'])\n",
    "    #.to_csv('files/nyse_index_data_monthly_2007-2022.csv', index=False)\n",
    ")"
   ]
  },
